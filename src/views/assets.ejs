<!DOCTYPE html>
<html lang="en">

<head>
    <% include partials/head %>
</head>

<body id="body-home" onload="getAssets()">
    <div id="sidemenu-section">
        <% include partials/menubar %>
    </div>

    <div class="page-content container">
        <section id="header-assets">
            <header class="container">
                <i id="assets-icon" class="huge icons">
                    <i class="images icon"></i>
                    <i class="corner inverted cog icon"></i>
                </i>
                <h1 class="title ui icon centered header">Assets Manager</h1>
            </header>
        </section>
        <section id="assets-table">
            <div id="nodevices-message" class="ui icon error hidden message">
                <i class="times icon"></i>
                <div class="content">
                    <div class="header">
                        No devices
                    </div>
                </div>
            </div>
            <table class="ui sortable teal celled table">
                <thead>
                    <tr>
                        <th>Asset</th>
                        <th>Status</th>
                        <th>Actions</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>
                            <h4 class="ui image header">
                                <img src="https://previews.123rf.com/images/naddya/naddya1406/naddya140600004/28904692-seamless-background-with-raspberry-vector-illustration.jpg"
                                    class="ui mini rounded image">
                                <div class="content">
                                    RaspBerry Default Image
                                </div>
                            </h4>
                        </td>
                        <td>Active</td>
                        <td class="center aligned action-group">
                            <div class="table-action" data-tooltip="Delete asset" data-position="top center" data-variation="basic">
                                <i class="large delete link icon"></i>
                            </div>
                            <div class="table-action" data-tooltip="Edit asset" data-position="top center" data-variation="basic">
                                <i class="large edit link icon"></i>
                            </div>
                        </td>
                    </tr>
                </tbody>
                <tfoot>
                    <tr>
                        <th>1 assets</th>
                        <th></th>
                        <th></th>
                    </tr>
                </tfoot>
            </table>
        </section>
    </div>
</body>

</html>